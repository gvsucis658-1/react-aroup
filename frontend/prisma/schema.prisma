generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = ""
}

enum Location {
  ONSITE
  REMOTE
  HYBRID
}

enum JobType {
  FULL_TIME
  PART_TIME
  CONTRACT
  INTERNSHIP
}

model jobPost {
  id        Int      @id @default(autoincrement())
  title     String
  team      String
  location  Location
  type      JobType
  published Boolean  @default(true)
  deadline  DateTime
}
